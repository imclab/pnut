#: ../models/Article.php:145
msgid "Content can't be empty"
msgstr "Содержание не может быть пустым"

msgid "Title can't be empty"
msgstr "Заголовок не может быть пустым"

#: ../models/Article.php:148
msgid "Publish date is incorrect"
msgstr "Некорректная дата публикации"

#: ../models/Article.php:150
msgid "Archive date is incorrect"
msgstr "Некорректная дата архивации"

#: ../models/Article.php:153
msgid "Author is not found"
msgstr "Автор не найден"

#: ../models/Article.php:156
msgid "Topic is not found"
msgstr "Раздел не найден"

#: ../models/Article.php:159
msgid "Type must be either article or gallery"
msgstr "Тип статьи должен быть 'article' или 'gallery'"

#: ../models/Article.php:162
msgid "Language code is incorrect"
msgstr "Некорректный код языка"

#: ../models/Article.php:165
msgid "Original article is not found"
msgstr "Статья-оригинал не найдена"

#: ../models/Attachment.php:175
msgid "Incorrect MIME-type format"
msgstr "Некорректный тип MIME"

#: ../models/Attachment.php:177
msgid "Filename can't be empty"
msgstr "Имя файла должно быть задано"

#: ../models/Comment.php:66
msgid "Incorrect email address"
msgstr "Некорректный адрес электронной почты"

#: ../models/Topic.php:94
msgid "Name must not be empty"
msgstr "Имя должно быть задано"

#: ../models/Topic.php:96
msgid "Name must contain symbols from set: A-Z, a-z, 0-9, \"-\", \"_\""
msgstr "Имя должно состоять из следующего набора символов: A-Z, a-z, 0-9, \"-\", \"_\""

#: ../models/Topic.php:103
msgid "Default subtopic must be child of this topic"
msgstr "Подраздел по умолчанию должен быть в поддереве данного раздела"

#: ../models/Topic.php:108
msgid "Item's child can't be parent of a child"
msgstr "Объект не может быть родителем своего наследника"

#: ../models/Topic.php:110
msgid "Topic can't be parent of itself"
msgstr "Объект не может быть родителем самому себе"

msgid "Owner is not found"
msgstr "Владелец не найден"

msgid "Group is not found"
msgstr "Группа не найдена"

#: ../models/User.php:62
msgid "Login must not be empty"
msgstr "Логин должен быть задан"

msgid "Password must not be empty"
msgstr "Пароль должен быть задан"

#: ../models/User.php:64
msgid "Login must contain symbols from set: A-Z, a-z, 0-9, \"-\", \"_\""
msgstr "Логин должен состоять из следующих симовлов: A-Z, a-z, 0-9, \"-\", \"_\""

#: ../models/User.php:67
msgid "Username must not be empty"
msgstr "Имя пользователя должно быть задано"

#: ../models/User.php:73
msgid ""
"Role must be either \"guest\", \"user\", \"manager\", \"admin\" or "
"\"superuser\""
msgstr "Роль должна быть \"гость\", \"пользователь\", \"менеджер\", \"администратор\" или \"суперпользователь\" "

msgid "Passwords don't match"
msgstr "Пароли не совпадают"
